<%-layout("/layouts/boilerplate") %>
<body>
      <form method="GET" action="/listings/new">
    </form>
    <h2> &nbsp; &nbsp; all listings</h2>
    <div class="cards">
 <% for(let listing of listings){%>
      <div class="card">
      <a href="/listings/<%=listing._id %>">
         <img src="<%=listing.image%>" class="imge" alt="">	</a>
         <h6><%=listing.title%></h6>
         <h6>	&#8377; <%=listing.price%>/night</h6>
          <form method="POST" action="/listings/<%=listing._id%>?_method=DELETE">
             <button class="btn-style " id="dlt">Delete</button>
             <br>
             <br>
          </form>
      </div>
    <%}%>
    </div>
</body>
